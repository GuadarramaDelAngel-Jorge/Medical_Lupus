<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary">

<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primaryColor"
    tools:context=".Resultados">
    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <Button
            android:onClick="ViewRHome"
            android:id="@+id/res_home_button"
            style="@style/Widget.MaterialComponents.Button.UnelevatedButton.Icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawableLeft="@drawable/home1"
            android:text="Inicio"
            android:textSize="16sp"/>
        <Button
            android:onClick="ViewRAnalisis"
            android:id="@+id/res_analisis_button"
            style="@style/Widget.MaterialComponents.Button.UnelevatedButton.Icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawableLeft="@drawable/create1"
            android:text="Análisis diario"
            android:textSize="16sp"/>
        <Button
            android:onClick="ViewRResultados"
            android:id="@+id/res_resultados_button"
            style="@style/Widget.MaterialComponents.Button.UnelevatedButton.Icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawableLeft="@drawable/assignment1"
            android:text="Resultados"
            android:textSize="16sp"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout"
        app:layout_constraintStart_toStartOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">
        <Button
            android:onClick="ViewRRecordatorios"
            android:id="@+id/res_recordatorios_button"
            style="@style/Widget.MaterialComponents.Button.UnelevatedButton.Icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawableLeft="@drawable/done1"
            android:text="Recordatorios"
            android:textSize="16sp"/>
        <Button
            android:onClick="ViewRDAyuda"
            android:id="@+id/res_ayuda_button"
            style="@style/Widget.MaterialComponents.Button.UnelevatedButton.Icon"
            android:textSize="16sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawableLeft="@drawable/help1"
            android:text="Ayuda"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/RFechaAT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="Fecha actual:"
            android:textColor="@color/secondaryLightColor"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"/>
        <TextView
            android:id="@+id/RFATFecha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:textColor="@color/secondaryDarkColor"
            android:text="Fecha"
            android:textSize="20sp"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/RFConsultaT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="Fecha de consulta:"
            android:textColor="@color/secondaryLightColor"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"/>
        <EditText
            android:id="@+id/RFCTInput"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="24dp"
            android:hint="Dd/Mm/Aaaa"
            android:textColor="@color/secondaryColor"
            android:textColorHint="@color/secondaryDarkColor"
            android:ems="10"
            android:textSize="20sp"
            android:inputType="date"/>
    </LinearLayout>

    <Button
        android:id="@+id/res_exportar_boton"
        android:layout_width="330dp"
        android:layout_height="45dp"
        style="@style/Widget.MaterialComponents.Button.UnelevatedButton.Icon"
        android:textSize="18sp"
        android:layout_marginTop="10dp"
        android:onClick="ViewRExportar"
        android:text="Exportar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4"/>
    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/res_exportar_boton"
        android:orientation="horizontal"
        android:gravity="center">
        <Button
            android:id="@+id/BBuscarSintoma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Widget.MaterialComponents.Button.UnelevatedButton.Icon"
            android:textSize="18sp"
            android:onClick="ViewRBSintoma"
            android:text="Buscar Síntoma"/>
        <Button
            android:id="@+id/BBuscarResultado"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Widget.MaterialComponents.Button.UnelevatedButton.Icon"
            android:textSize="18sp"
            android:layout_marginLeft="31dp"
            android:onClick="ViewRBResultado"
            android:text="Buscar Resultado"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/SintomaText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Síntoma"
            android:textColor="@color/secondaryLightColor"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/ResultadoText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="80dp"
            android:text="Resultado"
            android:textColor="@color/secondaryLightColor"
            android:textSize="24sp"/>
    </LinearLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="371dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </TableLayout>
</android.support.constraint.ConstraintLayout>
</android.support.v4.widget.NestedScrollView>